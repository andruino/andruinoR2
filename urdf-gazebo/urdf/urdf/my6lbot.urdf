        <updateRate>25.0</updateRate>
        <leftJoints>rueda_eje_centro_izqda rueda_eje_izqda_abajo rueda_eje_izqda_arriba</leftJoints>
        <rightJoints>rueda_eje_centro_drcha rueda_eje_drcha_arriba rueda_eje_drcha_abajo</rightJoints>
        <wheelSeparation>1</wheelSeparation>
        <wheelDiameter>0.35</wheelDiameter>
        <torque>50000</torque>
      <!--<robotNamespace></robotNamespace> No estaba puesto-->
